{
	"name": "health-being-web",
	"private": true,
	"version": "0.0.0",
	"type": "module",
	"scripts": {
		"dev": "vite",
		"build": "tsc -b && vite build",
		"lint": "biome check .",
		"lint:fix": "biome check . --write",
		"preview": "vite preview",
		"prepare": "husky",
		"typecheck": "tsc -p tsconfig.app.json --noEmit && tsc -p tsconfig.node.json --noEmit",
		"validate": "npm run lint && npm run typecheck"
	},
	"lint-staged": {
		"*": "biome check --write"
	},
	"dependencies": {
		"@heroicons/react": "^2.2.0",
		"@reduxjs/toolkit": "^2.8.1",
		"@tailwindcss/vite": "^4.1.6",
		"axios": "^1.9.0",
		"biome": "^0.2.2",
		"react": "^19.1.0",
		"react-dom": "^19.1.0",
		"react-google-recaptcha": "^3.1.0",
		"react-redux": "^9.2.0",
		"react-router-dom": "^7.6.1",
		"tailwindcss": "^4.1.6"
	},
	"devDependencies": {
		"@biomejs/biome": "^2.3.7",
		"@types/react": "^19.1.2",
		"@types/react-dom": "^19.1.2",
		"@types/react-google-recaptcha": "^2.1.9",
		"@vitejs/plugin-react": "^4.4.1",
		"husky": "^9.1.7",
		"lint-staged": "^16.2.3",
		"typescript": "~5.8.3",
		"vite": "^6.3.5"
	}
}
